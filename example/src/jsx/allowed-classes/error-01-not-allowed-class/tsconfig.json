{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "plugins": [
      {
        "name": "tailwind-typescript-plugin",
        "globalCss": "./global.css",
        "enableLogging": true,
        "validation": {
          "allowedClasses": []
        }
      }
    ]
  },
  "include": [
    "example.tsx"
  ]
}
